        [   INFLECTED #infl &
                    [ 1DRA-AUX-OR-BYUNG-AUX-OR-INTRANSITIVE-VERB-LEX-VERB-OR-SONG-AUX-OR-TRANSTIVE-VERB-LEX-VERB-OR-YONG-AUX-FLAG na  ] ] ].

past-tense-rule-lex-rule := tense-marker-lex-rule-super &
  [ DTR.INFLECTED.1DUG-AUX-OR-MED-AUX-OR-YOD-AUX-OR-YOGMARED-AUX-OR-YOGRED-AUX-FLAG na,
    SYNSEM.LOCAL.CONT.HOOK.INDEX.E.TENSE past ].

future-tense-rule-lex-rule := tense-marker-lex-rule-super &
  [ DTR.INFLECTED.YOGMARED-AUX-OR-YOGRED-AUX-FLAG na,
    SYNSEM.LOCAL.CONT.HOOK.INDEX.E.TENSE future ].

pa-fut-lex-rule := future-tense-rule-lex-rule &
  [ DTR.INFLECTED.1DUG-AUX-OR-MAN-AUX-OR-RED-AUX-OR-YIN-AUX-FLAG na ].

gi-fut-lex-rule := future-tense-rule-lex-rule &
  [ DTR.INFLECTED.MED-AUX-OR-YOD-AUX-FLAG na ].

present-tense-rule-lex-rule := tense-marker-lex-rule-super &
  [ DTR.INFLECTED.MAN-AUX-OR-RED-AUX-OR-YIN-AUX-FLAG na ].

dgos-lex-lex-rule := tense-marker-lex-rule-super &
  [ SYNSEM.LOCAL.CONT.HOOK.INDEX.E.TENSE present ].

gis-lex-rule := tense-marker-lex-rule-super &
  [ SYNSEM.LOCAL.CONT.HOOK.INDEX.E [ TENSE present,
                                     ASPECT prog ] ].

;;; Form feature specified as finite.
;;;

mood-marker-lex-rule-super := add-only-no-ccont-rule & infl-lex-rule &
  [ SYNSEM.LOCAL.CAT.HEAD.FORM finite,
    INFLECTED #infl,
    DTR mood-marker-rule-dtr &
        [ SYNSEM.LOCAL.CAT.HEAD.FORM finite,
	  INFLECTED #infl &
                    [ YIN-AUX-FLAG na ] ] ].

bas-interr-lex-rule := interr-marker-rule-lex-rule &
  [ DTR.INFLECTED.1DUG-AUX-OR-1DUG-VERB-OR-MAN-AUX-OR-MED-AUX-OR-RED-AUX-OR-RED-VERB-OR-SONG-AUX-OR-SONG-VERB-OR-YIN-AUX-OR-YIN-VERB-OR-YOD-AUX-OR-YOD-VERB-OR-YOGMARED-AUX-OR-YOGRED-AUX-OR-YOGRED-VERB-OR-YONG-AUX-FLAG na ].

ngas-interr-lex-rule := interr-marker-rule-lex-rule &
  [ DTR.INFLECTED.1DUG-AUX-OR-1DUG-VERB-OR-BYUNG-AUX-OR-MAN-AUX-OR-MED-AUX-OR-RED-AUX-OR-RED-VERB-OR-YIN-AUX-OR-YIN-VERB-OR-YOD-AUX-OR-YOD-VERB-OR-YOGMARED-AUX-OR-YOGRED-AUX-OR-YOGRED-VERB-FLAG na ].

pas-interr-lex-rule := interr-marker-rule-lex-rule &
  [ SYNSEM.LOCAL.CAT.HEAD.FORM finite  , 
    DTR.INFLECTED.1DUG-AUX-OR-1DUG-VERB-OR-BYUNG-AUX-OR-SONG-AUX-OR-SONG-VERB-OR-YONG-AUX-FLAG na ].


gas-interr-lex-rule := interr-marker-rule-lex-rule &
  [ DTR.INFLECTED.BYUNG-AUX-OR-MAN-AUX-OR-MED-AUX-OR-RED-AUX-OR-RED-VERB-OR-SONG-AUX-OR-SONG-VERB-OR-YIN-AUX-OR-YIN-VERB-OR-YOD-AUX-OR-YOD-VERB-OR-YOGMARED-AUX-OR-YOGRED-AUX-OR-YOGRED-VERB-OR-YONG-AUX-FLAG na ].

corrob-marker-rule-lex-rule := mood-marker-lex-rule-super &
  [ SYNSEM.LOCAL.CONT.HOOK.INDEX [ E.MOOD corrob ] ].

emph-marker-rule-lex-rule := mood-marker-lex-rule-super & 
  [ SYNSEM.LOCAL.CONT.HOOK.INDEX [ E.MOOD emph ] ].

poss-marker-rule-lex-rule := mood-marker-lex-rule-super &
  [ SYNSEM.LOCAL.CONT.HOOK.INDEX [ E.MOOD possible] ].

apparent-marker-rule-lex-rule := mood-marker-lex-rule-super &
  [ SYNSEM.LOCAL.CONT.HOOK.INDEX [ E.MOOD apparent ] ].

interr-marker-rule-lex-rule := mood-marker-lex-rule-super &
  [ SYNSEM.LOCAL.CONT.HOOK.INDEX [ E.MOOD interr,
                                   SF ques ] ].

indic-marker-rule-lex-rule := mood-marker-lex-rule-super &
  [ SYNSEM.LOCAL.CONT.HOOK.INDEX [ E.MOOD indic,
				   SF prop ]].

aspect-marker-suffixes-lex-rule-super := add-only-no-ccont-rule & infl-lex-rule &
  [ INFLECTED #infl,
    SYNSEM.LOCAL.CAT [ HEAD.FORM finite ], 
    DTR aspect-marker-suffixes-rule-dtr &
        [ INFLECTED #infl ] ].

simple-perfect-lex-rule := aspect-marker-suffixes-lex-rule-super &
  [ SYNSEM.LOCAL.CONT.HOOK.INDEX.E.ASPECT prf ].

aspect-marker-prefixes-lex-rule-super := add-only-no-ccont-rule & infl-lex-rule & mood-marker-rule-dtr & nominalizer-marker-rule-dtr &
  [ INFLECTED #infl,
    DTR aux-lex &
        [ INFLECTED #infl &
                    [ 1DRA-AUX-OR-1DUG-AUX-OR-BYUNG-AUX-OR-MED-AUX-OR-MYONG-AUX-OR-RYGUIN-AUX-OR-SONG-AUX-OR-YOD-AUX-OR-YOGRED-AUX-OR-YONG-AUX-FLAG na ] ] ].

recent-aspect-lex-rule := aspect-marker-prefixes-lex-rule-super &
  [ SYNSEM.LOCAL.CONT.HOOK.INDEX.E.ASPECT rec ].

