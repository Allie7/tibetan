  [ SYNSEM.LOCAL.CONT.HOOK.INDEX.E.ASPECT prog ].

pros-lex-rule := aspect-marker-prefixes-lex-rule-super &
  [ SYNSEM.LOCAL.CONT.HOOK.INDEX.E.ASPECT pros ].

nominalizer-lex-rule-super := high-or-mid-nominalization-lex-rule & infl-lex-rule &
  [ INFLECTED #infl,
    DTR nominalizer-marker-rule-dtr &
        [ INFLECTED #infl ] ].

nominalization-lex-rule := nominalizer-lex-rule-super.

;Constraint on MC used to be part of matrix.tdl
;;it applies to all wo implementations, except for v2

;;;;;;;;;;;;;;;;;;;
;;; Phrasal Types
;;;;;;;;;;;;;;;;;;;

high-no-rel-nominalized-clause-phrase := unary-phrase &
  [ SYNSEM.LOCAL.CAT [ HEAD noun &
                            [ NMZ +,
                              MOD < > ],
                       VAL [ COMPS < >,
                             SUBJ < >,
                             SPR < >,
                             SPEC < > ] ],
    C-CONT [ RELS <!  !>,
             HCONS <!  !>,
             HOOK [ XARG #xarg,
                    LTOP #ltop ] ],
    ARGS < [ SYNSEM.LOCAL [ CAT [ HEAD verb &
                                       [ NMZ + ],
                                  VAL [ COMPS < >,
                                        SUBJ < >,
                                        SPR < >,
                                        SPEC < > ] ],
                            CONT.HOOK [ XARG #xarg,
                                        LTOP #ltop ] ] ] > ].

head-mod-poss-phrase-1 := basic-head-mod-phrase-simple & head-compositional & head-final &
  [ SYNSEM.LOCAL.CAT [ HEAD +np,
                       VAL.SPEC #spec ],
    HEAD-DTR.SYNSEM.LOCAL.CAT [ HEAD +np,
                                VAL.SPEC #spec,
                                POSSESSUM nonpossessive ],
    NON-HEAD-DTR.SYNSEM.LOCAL.CAT [ VAL.SPR < >,
                                    HEAD.POSSESSOR possessor-1 ] ].

;Constraint on MC used to be part of matrix.tdl
;;it applies to all wo implementations, except for v2

; Bare NP phrase.  Consider modifying the PRED value of the quantifier relation
; introduced to match the semantic effect of bare NPs in your language.

bare-np-phrase := basic-bare-np-phrase &
  [ C-CONT.RELS <! [ PRED "exist_q_rel" ] !>,
    SYNSEM.LOCAL [ CAT [ VAL.SPEC #spec,
                         HEAD.CASE #case ],
                   CONT.HOOK.INDEX.PNG #png ],
    HEAD-DTR.SYNSEM.LOCAL [ CAT [ VAL.SPEC #spec,
                                  HEAD.CASE #case ],
                            CONT.HOOK.INDEX.PNG #png ] ].

;;;;;;;;;;;;;;;;;;
;;; Coordination
;;;;;;;;;;;;;;;;;;

;;; Feature Resolution Rules

pass-up-png-coord-rule := bottom-coord-phrase &
  [ SYNSEM.LOCAL [ CAT.HEAD.CASE #case,
                   CONT.HOOK.INDEX.PNG #png ],
    NONCONJ-DTR.SYNSEM.LOCAL [ CAT.HEAD.CASE #case,
                               CONT.HOOK.INDEX.PNG #png ] ].

same-same-same-case-coord-rule := coord-phrase &
  [ SYNSEM.LOCAL.CAT.HEAD.CASE #case,
    LCOORD-DTR.SYNSEM.LOCAL.CAT.HEAD.CASE #case,
    RCOORD-DTR.SYNSEM.LOCAL.CAT.HEAD.CASE #case ].

;;; Coordination Strategy 1

;JC 2018 2-8 special inverse coordination rule. this is likely incorrect and dang truly means "with"
; this takes care of coordination up until past the conjunct

inv-monopoly-top-coord-rule := top-coord-rule &
  [ LCOORD-DTR.SYNSEM.LOCAL.COORD +,
    RCOORD-DTR.SYNSEM.LOCAL.COORD - ].

inv-monopoly-mid-coord-rule := mid-coord-rule &
  [ LCOORD-DTR.SYNSEM.LOCAL.COORD +,
    RCOORD-DTR.SYNSEM.LOCAL.COORD - ].
;

n1-same-same-same-top-coord-rule := basic-n-top-coord-rule & inv-monopoly-top-coord-rule & same-same-same-case-coord-rule &
  [ SYNSEM.LOCAL.COORD-STRAT "1" ].

n1-same-same-same-mid-coord-rule := basic-n-mid-coord-rule & inv-monopoly-mid-coord-rule & same-same-same-case-coord-rule &
  [ SYNSEM.LOCAL.COORD-STRAT "1" ].

n1-bottom-coord-rule := conj-last-bottom-coord-rule & n-bottom-coord-phrase & pass-up-png-coord-rule &
  [ SYNSEM.LOCAL.COORD-STRAT "1" ].

;;; Coordination Strategy 1

np1-same-same-same-top-coord-rule := basic-np-top-coord-rule & inv-monopoly-top-coord-rule & same-same-same-case-coord-rule &
  [ SYNSEM.LOCAL.COORD-STRAT "1" ].

np1-same-same-same-mid-coord-rule := basic-np-mid-coord-rule & inv-monopoly-mid-coord-rule & same-same-same-case-coord-rule &
  [ SYNSEM.LOCAL.COORD-STRAT "1" ].

np1-bottom-coord-rule := conj-last-bottom-coord-rule & np-bottom-coord-phrase & pass-up-png-coord-rule &
  [ SYNSEM.LOCAL.COORD-STRAT "1" ].

;;; Coordination Strategy 2

;;; EMB 2018-02-08 Paramaterized list type to restrict VP coordination to VPs that are either COMPS < > or are looking for non-verby complements.  This is probably a hack. The goal is to rule out coordination of auxiliaries.  May run afoul of coordination of sentential complement taking verbs.

non-v-list := list.

non-v-cons := olist & cons &
  [ FIRST [ LOCAL.CAT.HEAD +njrpcdmo ],
    REST  non-v-list ].

non-v-null := non-v-list & null.


vp2-top-coord-rule := basic-vp-top-coord-rule & apoly-top-coord-rule &
  [ SYNSEM.LOCAL [ CAT.VAL.COMPS non-v-list,
		   COORD-STRAT "2" ] ].

vp2-bottom-coord-rule := unary-bottom-coord-rule & vp-bottom-coord-phrase &
  [ SYNSEM.LOCAL [ COORD-STRAT "2",
                   COORD-REL.PRED "_and_coord_rel" ] ].
