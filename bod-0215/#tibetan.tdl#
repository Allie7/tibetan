possessor-lex-rule-1 := head-change-with-ccont-lex-rule &
  [ SYNSEM.LOCAL.CAT [ HEAD noun &
                            [ MOD.FIRST [ LOCAL [ CAT [ VAL.SPR < [ ] >,
                                                        HEAD +np &
                                                             [ PRON - ] ],
                                                  CONT.HOOK [ INDEX #possessum,
                                                              LTOP #lbl ] ],
                                          OPT - ],
                              POSSESSOR possessor-1 ],
                       VAL [ SPR #spr,
                             COMPS #comps,
                             SUBJ #subj,
                             SPEC #spec ] ],
    C-CONT [ HOOK #hook,
             RELS <! arg12-ev-relation &
                     [ PRED "poss_rel",
                       LBL #lbl,
                       ARG1 #possessum,
                       ARG2 #possessor ] !>,
             HCONS <!  !>,
             ICONS <!  !> ],
    DTR.SYNSEM.LOCAL [ CONT.HOOK #hook &
                                 [ INDEX #possessor ],
                       CAT.VAL [ SPR #spr,
                                 COMPS #comps,
                                 SUBJ #subj,
                                 SPEC #spec ] ] ].

