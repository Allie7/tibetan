; Type assigning empty mod list. Added to basic types for nouns, verbs and determiners.

non-mod-lex-item := lex-item &
  [ SYNSEM.LOCAL.CAT.HEAD.MOD < > ].

;;;;;;;;;;;;;;;;;;;;;;;;;
;;; Matrix Type Addenda
;;;;;;;;;;;;;;;;;;;;;;;;;

png :+ [ CONTRACTED bool,
    PER person,
    NUM number,
    GEND gender ].

head :+ [ PRON bool,
    AUX bool,
    PRD bool,
    NMZ bool,
    POSSESSOR poss,
    FORM form,
    INIT bool ].

noun :+ [ CASE case ].

coord-phrase :+ [ SYNSEM.LOCAL.CAT.HEAD.NMZ #nmz,
    LCOORD-DTR.SYNSEM.LOCAL.CAT.HEAD.NMZ #nmz,
    RCOORD-DTR.SYNSEM.LOCAL.CAT.HEAD.NMZ #nmz ].

unary-bottom-coord-rule :+ [ SYNSEM.LOCAL.CAT.HEAD.NMZ #nmz,
    ARGS < [ SYNSEM.LOCAL.CAT.HEAD.NMZ #nmz ] > ].

inflected :+ [ 1DUG-AUX-OR-BYUNG-AUX-OR-MED-AUX-OR-MYONG-AUX-OR-RYGUIN-AUX-OR-SONG-AUX-OR-SUPER-MAIN-VERB-LEX-VERB-OR-YOD-AUX-OR-YOGRED-AUX-OR-YONG-AUX-FLAG luk,
    1DUG-AUX-OR-BYUNG-AUX-OR-SONG-AUX-OR-YONG-AUX-FLAG luk,
    SIMPLE-PERFECT-FLAG luk,
    BYUNG-AUX-OR-SONG-AUX-OR-SONG-VERB-OR-SUPER-MAIN-VERB-LEX-VERB-OR-YONG-AUX-FLAG luk,
    1DRA-AUX-OR-1DUG-AUX-OR-BYUNG-AUX-OR-KYI-AUX-OR-MYONG-AUX-OR-RED-AUX-OR-RYGUIN-AUX-OR-SONG-AUX-OR-SUPER-MAIN-VERB-LEX-VERB-OR-YOGRED-AUX-OR-YONG-AUX-FLAG luk,
    1DRA-AUX-OR-YOGMARED-AUX-OR-YOGRED-AUX-FLAG luk,
    NON-EMPTY-ASPECT-PREFIX-MARKER-FLAG luk,
    TENSE-MARKER-FLAG luk,
    ASPECT-MARKER-PREFIXES-FLAG luk,
    1DUG-AUX-OR-MAN-AUX-OR-MED-AUX-OR-RED-AUX-OR-SONG-AUX-OR-YIN-AUX-OR-YOD-AUX-OR-YOGMARED-AUX-OR-YOGRED-AUX-OR-YONG-AUX-FLAG luk,
    POLARITY-MARKER-FLAG luk,
    GENITIVE-LEX-RULE-FLAG luk,
    CONTRACTED-RULE-FLAG luk,
    MED-AUX-OR-YOD-AUX-FLAG luk,
    1DUG-AUX-OR-YONG-AUX-FLAG luk,
    PRON-NUM-MARKER-FLAG luk,
    1DRA-AUX-OR-1DUG-AUX-OR-MED-AUX-OR-YOD-AUX-OR-YOGMARED-AUX-OR-YOGRED-AUX-FLAG luk,
    ASPECT-MARKER-SUFFIXES-FLAG luk,
    CASE-MARKER-FLAG luk,
    POSSESSIVE-RULE-LEX-FLAG luk,
    1DUG-AUX-OR-BYUNG-AUX-OR-MAN-AUX-OR-MED-AUX-OR-RED-AUX-OR-YIN-AUX-OR-YOD-AUX-OR-YOGMARED-AUX-OR-YOGRED-AUX-FLAG luk,
