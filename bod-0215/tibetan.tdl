infl-satisfied :+ [ 1DUG-AUX-OR-BYUNG-AUX-OR-MED-AUX-OR-MYONG-AUX-OR-RYGUIN-AUX-OR-SONG-AUX-OR-SUPER-MAIN-VERB-LEX-VERB-OR-YOD-AUX-OR-YOGRED-AUX-OR-YONG-AUX-FLAG na-or-+,
    1DUG-AUX-OR-BYUNG-AUX-OR-SONG-AUX-OR-YONG-AUX-FLAG na-or-+,
    SIMPLE-PERFECT-FLAG na-or-+,
    BYUNG-AUX-OR-SONG-AUX-OR-SONG-VERB-OR-SUPER-MAIN-VERB-LEX-VERB-OR-YONG-AUX-FLAG na-or-+,
    1DRA-AUX-OR-1DUG-AUX-OR-BYUNG-AUX-OR-KYI-AUX-OR-MYONG-AUX-OR-RED-AUX-OR-RYGUIN-AUX-OR-SONG-AUX-OR-SUPER-MAIN-VERB-LEX-VERB-OR-YOGRED-AUX-OR-YONG-AUX-FLAG na-or-+,
    1DRA-AUX-OR-YOGMARED-AUX-OR-YOGRED-AUX-FLAG na-or-+,
    NON-EMPTY-ASPECT-PREFIX-MARKER-FLAG na-or-+,
    TENSE-MARKER-FLAG na-or-+,
    ASPECT-MARKER-PREFIXES-FLAG na-or-+,
    1DUG-AUX-OR-MAN-AUX-OR-MED-AUX-OR-RED-AUX-OR-SONG-AUX-OR-YIN-AUX-OR-YOD-AUX-OR-YOGMARED-AUX-OR-YOGRED-AUX-OR-YONG-AUX-FLAG na-or-+,
    POLARITY-MARKER-FLAG na-or-+,
    GENITIVE-LEX-RULE-FLAG na-or-+,
    CONTRACTED-RULE-FLAG na-or-+,
    MED-AUX-OR-YOD-AUX-FLAG na-or-+,
    1DUG-AUX-OR-YONG-AUX-FLAG na-or-+,
    PRON-NUM-MARKER-FLAG na-or-+,
    1DRA-AUX-OR-1DUG-AUX-OR-MED-AUX-OR-YOD-AUX-OR-YOGMARED-AUX-OR-YOGRED-AUX-FLAG na-or-+,
    ASPECT-MARKER-SUFFIXES-FLAG na-or-+,
    CASE-MARKER-FLAG na-or-+,
    POSSESSIVE-RULE-LEX-FLAG na-or-+,
    1DUG-AUX-OR-BYUNG-AUX-OR-MAN-AUX-OR-MED-AUX-OR-RED-AUX-OR-YIN-AUX-OR-YOD-AUX-OR-YOGMARED-AUX-OR-YOGRED-AUX-FLAG na-or-+,
    MOOD-MARKER-FLAG na-or-+,
    NOUN-FLAG na-or-+,
    YIN-AUX-OR-YIN-VERB-OR-YOD-AUX-OR-YOD-VERB-OR-YOGRED-AUX-OR-YOGRED-VERB-FLAG na-or-+,
    BARE-ABS-RULE-OR-DATIVE-LEX-RULE-OR-ERGATIVE-LEX-RULE-OR-INSTRUMENTAL-LEX-RULE-FLAG na-or-+,
    1DRA-AUX-FLAG na-or-+,
    1DRA-AUX-OR-MAN-AUX-OR-RED-AUX-OR-YIN-AUX-FLAG na-or-+,
    NON-EMPTY-TENSE-MARKER-RULE-FLAG na-or-+,
    BYUNG-AUX-OR-MAN-AUX-OR-MED-AUX-OR-RED-AUX-OR-SONG-AUX-OR-YIN-AUX-OR-YOD-AUX-OR-YOGMARED-AUX-OR-YOGRED-AUX-OR-YONG-AUX-FLAG na-or-+,
    1DUG-AUX-OR-MAN-AUX-OR-RED-AUX-OR-YIN-AUX-FLAG na-or-+,
    BYUNG-AUX-OR-RED-AUX-OR-SONG-AUX-FLAG na-or-+,
    3RD_PERSON_F_LEX-NOUN-FLAG na-or-+ ].

cat :+ [ POSSESSUM poss ].

poss := *top* &
  [ POSS-AGR png ].

possessive := poss.

nonpossessive := poss.

possessor := possessive.

possessum := possessive.

basic-bare-np-phrase :+ [ SYNSEM.LOCAL.CAT [ HEAD.POSSESSOR #possessor,
                       POSSESSUM #possessum ],
    HEAD-DTR.SYNSEM.LOCAL.CAT [ HEAD.POSSESSOR #possessor,
                                POSSESSUM #possessum ] ].

possessive-1 := possessive.

possessor-1 := possessor & possessive-1.

possessum-1 := possessum & possessive-1.

basic-head-comp-phrase :+ [ SYNSEM.LOCAL.CAT.MC #mc,
    HEAD-DTR.SYNSEM.LOCAL.CAT.MC #mc ].

basic-head-opt-subj-phrase :+ [ HEAD-DTR.SYNSEM.LOCAL.CAT.VAL.COMPS < > ].

head-adj-int-phrase :+ [ NON-HEAD-DTR.SYNSEM.LOCAL.CAT [ HEAD.POSSESSOR nonpossessive,
                                    POSSESSUM nonpossessive ] ].

;Added for adverbs
adj-head-int-phrase := adj-head-phrase & isect-mod-phrase &
  [ NON-HEAD-DTR.SYNSEM.LOCAL.CAT [ POSTHEAD - ] ].


adj-head-int-phrase :+ [ NON-HEAD-DTR.SYNSEM.LOCAL.CAT [ HEAD.POSSESSOR nonpossessive,
                                    POSSESSUM nonpossessive ] ].
;



adj-head-scop-phrase :+ [ NON-HEAD-DTR.SYNSEM.LOCAL.CAT [ HEAD.POSSESSOR nonpossessive,
                                    POSSESSUM nonpossessive ] ].

possessive-pron-1 := possessive.

possessor-pron-1 := possessor & possessive-pron-1.

basic-head-mod-phrase-simple :+ [ SYNSEM.LOCAL.CAT.MC #mc,
    HEAD-DTR.SYNSEM.LOCAL.CAT.MC #mc ].

;;;;;;;;;;;;;;
;;; Features
;;;;;;;;;;;;;;

;;; Case

case := *top*.
dat := case.  ; dative
ins := case.  ; instrumental
gen := case.  ; genitive
abs+erg := case.
abs := abs+erg.
erg := abs+erg.

;;; Person

person := *top*.
1st := person.
2nd := person.
3rd := person.

;;; Number

number := *top*.
sg := number.
pl := number.

;;; Gender

gender := *top*.
m := gender.
f := gender.

;Constraint on MC used to be part of matrix.tdl
;;it applies to all wo implementations, except for v2

comp-head-phrase := basic-head-1st-comp-phrase & head-final &
  [ HEAD-DTR.SYNSEM.LOCAL.CAT.HEAD.INIT - ].

subj-head-phrase := decl-head-subj-phrase & head-final &
  [ HEAD-DTR.SYNSEM.LOCAL.CAT.VAL.COMPS < > ].

; JC working on wh questions 2-11

eng-subj-head-phrase := head-valence-phrase & head-compositional & 
              basic-binary-headed-phrase &
  [ SYNSEM phr-synsem & 
           [ LOCAL.CAT [ POSTHEAD +,
                 HC-LIGHT -,
                 VAL [ SUBJ < >,
                       COMPS #comps,
                       SPR #spr ] ] ],
    C-CONT [ HOOK.INDEX.SF prop-or-ques,
         RELS <! !>,
         HCONS <! !>,
         ICONS <! !> ],
    HEAD-DTR.SYNSEM.LOCAL.CAT.VAL [ SUBJ < #synsem >,
                    COMPS #comps,    
                    SPR #spr ],
    NON-HEAD-DTR.SYNSEM #synsem & canonical-synsem &
       [ LOCAL [ CAT [ VAL [ SUBJ olist,
                 COMPS olist,
                 SPR olist ] ] ],
         NON-LOCAL [ SLASH 0-dlist & [ LIST < > ],
             REL 0-dlist ] ]].


subj-head-phrase := eng-subj-head-phrase & head-final &
  [ HEAD-DTR.SYNSEM.LOCAL.CAT.VAL.COMPS < > ].


;

;;; Form

form := *top*.
nonfinite := form.
finite := form.

inner1 := form.
inner2 := inner1.
aux1 := form.
aux3 := inner2.
mv := inner2.
aux2 := inner1.

inner1-fin := inner1 & finite.
inner2-fin := inner2 & inner1-fin.
aux2-fin := inner1-fin & aux2.
aux3-fin := inner2-fin & aux3.
mv-fin := inner2-fin & mv.
aux1-fin := aux1 & finite.

inner1-inf := inner1 & nonfinite.
inner2-inf := inner2 & inner1-inf.
aux2-inf := inner1-inf & aux2.
aux3-inf := inner2-inf & aux3.
mv-inf := inner2-inf & mv.
aux1-inf := aux1 & nonfinite.


;;; Tense

past := tense.
present := tense.
future := tense.

;;; Aspect

prf := aspect.
rec := aspect.
pros := aspect.
prog := aspect.

;;; Mood

indic := mood.
interr := mood.
emph := mood.
corrob := mood.
wh-interr := mood.

;;;;;;;;;;;;;;;;;;;
;;; Lexical Types
;;;;;;;;;;;;;;;;;;;

;;; Wh pronouns

;; JC 2-11 for wh questions

wh-pronoun-noun-lex := case-marker-rule-dtr & possessive-rule-lex-rule-dtr & norm-hook-lex-item & basic-icons-lex-item & 
  [ SYNSEM [ LOCAL [ CAT [ HEAD noun,
			   VAL [ SPR < >,
				 SUBJ < >,
				 COMPS < >,
				 SPEC < > ] ],
		     CONT [ HOOK.INDEX.PNG.PER 3rd,
	                    RELS <! [ LBL #larg,
				       ARG0 #ind & ref-ind ],
				  [ PRED "wh_q_rel",
				    ARG0 #ind,
				    RSTR #harg ] !>,
			    HCONS <! [ HARG #harg,
				        LARG #larg ] !> ] ],
	     NON-LOCAL.QUE <! #ind !> ] ].


wh-int-cl := clause & head-compositional &  head-only &
  [ SYNSEM [ LOCAL.CAT [ VAL #val,
			 MC bool ],
	     NON-LOCAL non-local-none ],
    C-CONT [ RELS <! !>,
	     HCONS <! !>,
	     HOOK.INDEX.SF ques ],
    HEAD-DTR.SYNSEM [ LOCAL.CAT [ HEAD verb & [ FORM finite ],
				  VAL #val & 
				    [ SUBJ < >,
				      COMPS < > ] ],
		      NON-LOCAL [ SLASH <! !>,
				  REL <! !>,
				  QUE <! ref-ind !> ] ] ].

;end


;;; Nouns

noun-lex := basic-noun-lex & basic-one-arg & no-hcons-lex-item & non-mod-lex-item & case-marker-rule-dtr & possessive-rule-lex-rule-dtr &
  [ ARG-ST < #spr >,
    SYNSEM.LOCAL.CAT [ VAL [ SPR < #spr &
                                   [ LOCAL.CAT.HEAD det ] >,
                             COMPS < >,
                             SUBJ < >,
                             SPEC < > ],
                       HEAD.NMZ - ],
    INFLECTED [ GENITIVE-LEX-RULE-FLAG na-or--,
                BARE-ABS-RULE-OR-DATIVE-LEX-RULE-OR-ERGATIVE-LEX-RULE-OR-INSTRUMENTAL-LEX-RULE-FLAG na-or--,
                NOUN-FLAG + ] ].

no-spr-noun-lex := noun-lex &
  [ SYNSEM.LOCAL.CAT.VAL.SPR < [ OPT + ] > ].

; Nouns which cannot take specifiers mark their SPR requirement
; as OPT +.  Making the non-head daughter OPT - in this rule
; keeps such nouns out.
; 
; Rules for building NPs.  Note that the Matrix uses SPR for
; the specifier of nouns and SUBJ for the subject (specifier) of verbs.

head-spec-phrase := basic-head-spec-phrase & head-initial &
  [ NON-HEAD-DTR.SYNSEM [ OPT -,
                          LOCAL.CAT [ VAL.SPR < >,
                                      HEAD +vjrcdmo &
                                           [ POSSESSOR nonpossessive ] ] ] ].

common_noun_lex-noun-lex := noun-lex & pron-num-marker-rule-dtr &
  [ SYNSEM.LOCAL.CONT.HOOK.INDEX.PNG [ PER 3rd,
                                       NUM sg ],
    INFLECTED [ PRON-NUM-MARKER-FLAG -,
                CASE-MARKER-FLAG -,
                POSSESSIVE-RULE-LEX-FLAG - ] ].

pron-noun-lex := noun-lex & pron-num-marker-rule-dtr &
  [ SYNSEM.LOCAL.CONT.HOOK.INDEX [ COG-ST activ-or-more,
				   SPECI + ] ].

1st_person_lex-noun-lex := no-spr-noun-lex & pron-noun-lex &
  [ SYNSEM.LOCAL [ CAT.HEAD.PRON +,
                   CONT.HOOK.INDEX.PNG.PER 1st ],
    INFLECTED [ PRON-NUM-MARKER-FLAG -,
                CASE-MARKER-FLAG -,
                POSSESSIVE-RULE-LEX-FLAG - ] ].

3rd_person_m_lex-noun-lex := no-spr-noun-lex & pron-noun-lex &
  [ SYNSEM.LOCAL [ CAT.HEAD.PRON +,
                   CONT.HOOK.INDEX.PNG [ GEND m,
                                         PER 3rd ] ],
    INFLECTED [ PRON-NUM-MARKER-FLAG -,
                CASE-MARKER-FLAG -,
                POSSESSIVE-RULE-LEX-FLAG - ] ].

2nd_person_lex-noun-lex := no-spr-noun-lex & pron-noun-lex &
  [ SYNSEM.LOCAL [ CAT.HEAD.PRON +,
                   CONT.HOOK.INDEX.PNG.PER 2nd ],
    INFLECTED [ PRON-NUM-MARKER-FLAG -,
                CASE-MARKER-FLAG -,
                POSSESSIVE-RULE-LEX-FLAG - ] ].

proper_noun_lex-noun-lex := no-spr-noun-lex &
  [ SYNSEM.LOCAL.CONT.HOOK.INDEX.PNG [ PER 3rd,
                                       NUM sg ],
    INFLECTED [ CASE-MARKER-FLAG -,
                POSSESSIVE-RULE-LEX-FLAG - ] ].

3rd_person_f_lex-noun-lex := no-spr-noun-lex & pron-noun-lex &
  [ SYNSEM.LOCAL [ CAT.HEAD.PRON +,
                   CONT.HOOK.INDEX.PNG [ PER 3rd,
                                         GEND f ] ],
    INFLECTED [ PRON-NUM-MARKER-FLAG -,
                3RD_PERSON_F_LEX-NOUN-FLAG +,
                CASE-MARKER-FLAG -,
                POSSESSIVE-RULE-LEX-FLAG - ] ].

;;; Verbs

verb-lex-super := non-mod-lex-item & aspect-marker-prefixes-rule-dtr & aspect-marker-suffixes-rule-dtr & mood-marker-rule-dtr & nominalizer-rule-dtr & polarity-marker-rule-dtr & tense-marker-rule-dtr.

verb-lex := verb-lex-super &
  [ SYNSEM.LOCAL.CAT.HEAD verb &
                          [ NMZ - ],
    INFLECTED [ MOOD-MARKER-FLAG -,
                ASPECT-MARKER-SUFFIXES-FLAG -,
                POLARITY-MARKER-FLAG -,
                TENSE-MARKER-FLAG -,
                ASPECT-MARKER-PREFIXES-FLAG - ] ].

main-verb-lex := verb-lex & basic-verb-lex &
  [ SYNSEM.LOCAL [ CAT [ HEAD.AUX -,
                         VAL [ SPR < >,
                               SPEC < >,
                               SUBJ < #subj > ] ],
                   CONT.HOOK.XARG #xarg ],
    ARG-ST.FIRST #subj &
                 [ LOCAL [ CAT cat-sat &
                               [ VAL [ SPR < >,
                                       COMPS < > ] ],
                           CONT.HOOK.INDEX #xarg ] ] ].


;
; No argument optionality for aux verbs
; JC 02-11
aux-lex := verb-lex &
  [ SYNSEM.LOCAL.CAT [ HEAD.AUX +,
		       VAL.COMPS < [OPT - ,
				    LOCAL.CAT.HEAD.NMZ - ] > ],
    INFLECTED [ MOOD-MARKER-FLAG -,
                ASPECT-MARKER-SUFFIXES-FLAG -,
                POLARITY-MARKER-FLAG -,
                TENSE-MARKER-FLAG -,
                ASPECT-MARKER-PREFIXES-FLAG - ] ].

intransitive-verb-lex := main-verb-lex & intransitive-lex-item &
  [ SYNSEM.LOCAL.CAT.VAL.COMPS < >,
    ARG-ST.FIRST.LOCAL.CAT.HEAD noun ].


;attempt to cut down on overgeneration by specifying tense didn't work
;  [ SYNSEM.LOCAL.CONT.HOOK.INDEX.E.TENSE past,
transitive-verb-lex := main-verb-lex & transitive-lex-item &
  [ SYNSEM.LOCAL.CAT.VAL.COMPS < #comps >,
    ARG-ST < [ LOCAL.CAT.HEAD noun ],
             #comps &
             [ LOCAL.CAT cat-sat &
                         [ VAL [ SPR < >,
                                 COMPS < > ],
                           HEAD noun ] ] > ].

clausal-verb-lex := main-verb-lex &
  [ SYNSEM.LOCAL.CAT.VAL.COMPS < #comps >,
    ARG-ST < [ LOCAL.CAT.HEAD noun ],
             #comps &
             [ LOCAL.CAT.VAL [ SPR < >,
                               COMPS < >,
                               SUBJ < > ] ] > ].

abs-intransitive-verb-lex := intransitive-verb-lex &
  [ ARG-ST.FIRST.LOCAL.CAT.HEAD noun &
                                [ CASE abs ] ].

erg-abs-transitive-verb-lex := transitive-verb-lex &
  [ ARG-ST < [ LOCAL.CAT.HEAD noun &
                              [ CASE erg ] ],
             [ LOCAL.CAT.HEAD noun &
                              [ CASE abs ] ] > ].

clausal-erg-abs-verb-lex := clausal-verb-lex &
  [ ARG-ST < [ LOCAL.CAT.HEAD noun &
                              [ CASE erg ] ],
             [ LOCAL.CAT.HEAD noun &
                              [ CASE abs ] ] > ].

clausal-erg-verb-lex := clausal-verb-lex &
  [ ARG-ST.FIRST.LOCAL.CAT.HEAD noun &
                                [ CASE erg ] ].

intransitive-verb-lex-verb-lex := super-main-verb-lex-verb-lex & abs-intransitive-verb-lex.

transitive-erg-verb-lex-verb-lex := transtive-verb-lex-verb-lex &
  [ SYNSEM.LOCAL.CAT.VAL [ SUBJ.FIRST.LOCAL.CAT.HEAD.CASE erg,
                           COMPS.FIRST.LOCAL.CAT.HEAD.CASE abs ] ].

obj-dat-trans-verb-lex-verb-lex := transtive-verb-lex-verb-lex &
  [ SYNSEM.LOCAL.CAT.VAL [ SUBJ.FIRST.LOCAL.CAT.HEAD.CASE abs+erg,
                           COMPS.FIRST.LOCAL.CAT.HEAD.CASE dat ] ].

transtive-verb-lex-verb-lex := super-main-verb-lex-verb-lex & transitive-verb-lex &
  [ SYNSEM.LOCAL.CAT.VAL.COMPS.FIRST.OPT - ].

abs-dat-transitive-verb-lex := transtive-verb-lex-verb-lex &
  [ SYNSEM.LOCAL.CAT.VAL [ SUBJ.FIRST.LOCAL.CAT.HEAD.CASE abs,
                           COMPS.FIRST.LOCAL.CAT.HEAD.CASE dat ] ].

; JC added no arg opt
yin-verb-lex := transitive-verb-lex &
  [ SYNSEM.LOCAL.CAT.VAL.COMPS.FIRST.OPT - ] & 
  [ INFLECTED [ YIN-AUX-OR-YIN-VERB-OR-YOD-AUX-OR-YOD-VERB-OR-YOGRED-AUX-OR-YOGRED-VERB-FLAG +,
                CONTRACTED-RULE-FLAG + ] ].

; JC added no arg opt

red-verb-lex := transitive-verb-lex &
  [ SYNSEM.LOCAL.CAT.VAL.COMPS.FIRST.OPT - ] &  
  [ INFLECTED.CONTRACTED-RULE-FLAG + ].

yod-verb-lex := intransitive-verb-lex &
  [ INFLECTED [ YIN-AUX-OR-YIN-VERB-OR-YOD-AUX-OR-YOD-VERB-OR-YOGRED-AUX-OR-YOGRED-VERB-FLAG +,
		CONTRACTED-RULE-FLAG + ] ].

1dug-verb-lex := intransitive-verb-lex & 
  [ INFLECTED.CONTRACTED-RULE-FLAG + ].

yogred-verb-lex := intransitive-verb-lex &
  [ INFLECTED [ YIN-AUX-OR-YIN-VERB-OR-YOD-AUX-OR-YOD-VERB-OR-YOGRED-AUX-OR-YOGRED-VERB-FLAG + ,
		CONTRACTED-RULE-FLAG + ] ].

man-verb-lex :=  transitive-verb-lex &
  [ INFLECTED [ YIN-AUX-OR-YIN-VERB-OR-YOD-AUX-OR-YOD-VERB-OR-YOGRED-AUX-OR-YOGRED-VERB-FLAG +,
                CONTRACTED-RULE-FLAG - ] ].

med-verb-lex := intransitive-verb-lex &
  [ INFLECTED [ YIN-AUX-OR-YIN-VERB-OR-YOD-AUX-OR-YOD-VERB-OR-YOGRED-AUX-OR-YOGRED-VERB-FLAG +,
		CONTRACTED-RULE-FLAG - ] ].

yogmared-verb-lex := intransitive-verb-lex &
  [ INFLECTED [ YIN-AUX-OR-YIN-VERB-OR-YOD-AUX-OR-YOD-VERB-OR-YOGRED-AUX-OR-YOGRED-VERB-FLAG +,
		CONTRACTED-RULE-FLAG - ] ].

song-verb-lex := abs-intransitive-verb-lex &
  [ SYNSEM.LOCAL.CONT.HOOK.INDEX.E.TENSE past,
    INFLECTED [ BYUNG-AUX-OR-SONG-AUX-OR-SONG-VERB-OR-SUPER-MAIN-VERB-LEX-VERB-OR-YONG-AUX-FLAG + ,
		CONTRACTED-RULE-FLAG + ] ].

comp-accept-verb-lex := super-main-verb-lex-verb-lex & clausal-verb-lex & clausal-second-arg-trans-lex-item &
  [ SYNSEM.LOCAL [ CONT.HOOK.INDEX.E.TENSE present,
                   CAT.VAL.COMPS < [ LOCAL.CAT.HEAD noun &
                                                    [ NMZ + ] ] > ],
    INFLECTED.CONTRACTED-RULE-FLAG + ].

;Constraining unspecified transitive for generation purposes

unspec-trans-verb-lex := transitive-verb-lex &
  [ SYNSEM.LOCAL.CAT.VAL [ SUBJ.FIRST.LOCAL.CAT.HEAD.CASE abs,
                           COMPS.FIRST.LOCAL.CAT.HEAD.CASE abs ] ,
    INFLECTED.CONTRACTED-RULE-FLAG + ] &
  [ SYNSEM.LOCAL [ CONT.HOOK.INDEX.E.TENSE past,
                   CAT.VAL [ SUBJ.FIRST.OPT -,
                             COMPS.FIRST.OPT - ] ] ].
